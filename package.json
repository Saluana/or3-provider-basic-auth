{
  "name": "or3-provider-basic-auth",
  "version": "0.0.1",
  "description": "Basic-authentication provider for OR3 Chat with SSR session resolution, secure cookies, and provider-owned auth UI.",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs"
    },
    "./nuxt": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs"
    }
  },
  "main": "./dist/module.mjs",
  "types": "./dist/types.d.mts",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/types.d.mts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "build": "nuxt-module-build build",
    "lint": "eslint \"src/**/*.{ts,d.ts,vue}\"",
    "type-check": "tsc -p tsconfig.typecheck.json --noEmit",
    "test": "vitest run"
  },
  "dependencies": {
    "bcryptjs": "^3.0.3",
    "better-sqlite3": "^12.4.1",
    "jsonwebtoken": "^9.0.2",
    "zod": "^4.1.12"
  },
  "peerDependencies": {
    "@nuxt/kit": "^3.0.0",
    "nuxt": "^3.0.0 || ^4.0.0"
  },
  "devDependencies": {
    "@nuxt/module-builder": "^1.0.2",
    "@types/better-sqlite3": "^7.6.13",
    "@types/bun": "latest",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^25.2.1",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^9.39.2",
    "eslint-plugin-vue": "^10.7.0",
    "h3": "^1.15.4",
    "jsdom": "^28.0.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18",
    "vue": "^3.5.22",
    "vue-eslint-parser": "^10.2.0",
    "vue-tsc": "^2.2.10"
  }
}
